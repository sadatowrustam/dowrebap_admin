                    <div class="information change"><%=location[i].email%></div>
                  </div>
                <%}}%>
              </div>
            <% } %>
          </div>
          
          <% if (list.banner[2].banner != null) { %>
          <div class="splide3">
            <div class="splide__track">
              <ul class="splide__list">
                    <%for(var i = 0; i < list.banner[2].banner.length; i++){%>
                      <li class="splide__slide"><a target="_blank" href="//<%= list.banner[2].banner[i].link %>"><div><img src="<%= host %>/banners/<%=list.banner[2].banner[i].pic%>" alt=""></div></a></li>
                    <%}%>
                  </ul>
                </div>
              </div>
            </div>
          <% } %>
      </section>

      <!-- map derdi -->
      <script>
        //map
        const tm = document.querySelectorAll('.tm svg path');
        const info = document.querySelectorAll(".info .velayat");
        info[0].style.display="block";
        for(var i = 0; i<tm.length; i++){
          tm[i].addEventListener("mouseover",async function(){
            for(var j = 0; j<tm.length; j++){
              tm[j].classList.remove('active');
              info[j].style.display = "none";
            }
            this.classList.add('active');
            info[this.id].style.display = "block"
          });
        };

        var welayat = document.querySelectorAll(".welayatNames div");

        for(var i =0; i<welayat.length; i++){
          welayat[i].addEventListener("click",function(){
            for(var j = 0; j<welayat.length; j++){
              welayat[j].style.borderBottom = "none";
            }
            for(var j = 0; j<info.length; j++){
              info[j].style.display = "none";
              this.style.borderBottom = "3px solid #2058D5";
            }
            switch (Number(this.className)){
              case 0 :
                info[0].style.display="block";
                break;
              case(1):
                info[0].style.display ="block";
                break;
              case(2):
                info[2].style.display ="block";
                break
              case(3):
                info[4].style.display ="block";
                break
              case(4):
                info[3].style.display ="block";
                break
              case(5):
                info[1].style.display ="block";
                break
            }
          })
        }
      </script>


      <!-- splide -->
      <script>
        document.addEventListener('DOMContentLoaded', function() {

        new Splide(".splide3",{
          type:"loop",
          perPage:1,
          autoplay:true,
          cover:true
        }).mount();
      });
      </script>
</body>
