                      <div class="head change" id="mapHead" data-dynamic data-tm="Ahal" data-ru="Ахал" data-en="Ahal">Ahal</div>
                      <div class="title" id="mapAdress" data-dynamic data-tm="Salgysy" data-en="Address" data-ru="Адрес">Adress:</div>
                      <div class="information change" id="mapLocation" data-dynamic data-tm="<%=location[i].tmAddress%>" data-ru="<%=location[i].ruAddress%>" data-en="<%=location[i].enAddress%>"><%=location[i].tmAddress%></div>
                      <div class="title" data-dynamic data-tm="Kabulhananyň telefon belgileri" data-en="Reception phones" data-ru="Телефоны приемной" id="mapPhones">Telefonlar:</div>
                      <%for(var j = 0; j<location[i].phone.length; j++){%>
                        <div class="information change up"><%=location[i].phone[j]%></div>
                      <%}%>
                      <div class="title" data-dynamic data-tm="Faks" data-en="Fax" data-ru="Факс" id="mapFaks">Faks:</div>
                      <div class="information change faks"><%=location[i].faks%></div>
                      <div class="title" data-dynamic data-tm="E-mail" data-en="E-mail" data-ru="Email" id="mapEmail">Elektron pochta:</div>
                      <div class="information change"><%=location[i].email%></div>
  
                    </div>
                    
                    
                  <% }else{ %>
                  <div class="velayat" style="display: none;">
                    <div class="head change" id="mapHead" data-dynamic data-tm="<%=location[6][i-2]%>" data-ru="<%=location[7][i-2]%>" data-en="<%=location[8][i-2]%>"><%=location[7][i-2]%></div>
