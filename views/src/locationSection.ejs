            <% if (list.location !=null) { %>
              <div class="info">
                <%for(var i = 1; i<6  ;i++){%>
                  <% if (i==1) { %>
                    <div class="velayat" style="display: none;">
  
                      <div class="head change" id="mapHead" data-dynamic data-tm="Aşgabat" data-ru="Ашхабад" data-en="Ashgabat">Ashgabat</div>
                      <div class="title" id="mapAdress" data-dynamic data-tm="Salgysy" data-en="Address" data-ru="Адрес">Adress:</div>
                      <div class="information change" id="mapLocation" data-dynamic data-tm="<%=location[i-1].tmAddress%>" data-ru="<%=location[i-1].ruAddress%>" data-en="<%=location[i-1].enAddress%>"><%=location[i-1].tmAddress%></div>
                      <div class="title" data-dynamic data-tm="Kabulhananyň telefon belgileri" data-en="Reception phones" data-ru="Телефоны приемной" id="mapPhones">Telefonlar:</div>
                      <%for(var j = 0; j<location[i-1].phone.length; j++){%>
                        <div class="information change up"><%=location[i-1].phone[j]%></div>
                      <%}%>
                      <div class="title" id="mapFaks" data-dynamic data-tm="Faks" data-en="Fax" data-ru="Факс">Faks:</div>
                      <div class="information change faks"><%=location[i-1].faks%></div>
                      <div class="title" id="mapEmail" data-dynamic data-tm="E-mail" data-en="E-mail" data-ru="Email">Elektron pochta:</div>
